{
    "manifest_version": 3,
    "name": "YouTube Redirect",
    "version": "1.0",
    "description": "Redirects YouTube homepage and shorts to Subscriptions page",
    "author": "Tarang Chikhalia",
    "permissions": [
      "declarativeNetRequestWithHostAccess",
      "declarativeNetRequestFeedback"
    ],
    "host_permissions": [
        "*://*.youtube.com/*"
    ],
    "declarative_net_request" : {
      "rule_resources" : [{
          "id": "ruleset_1",
          "enabled": true,
          "path": "redirect_rule.json"
        }
      ]
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "YouTube Redirect"
    }
  }
  